#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/osman/pardus-dev/node_modules/.pnpm/@roxi+routify@2.18.11/node_modules/@roxi/routify/plugins/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/@roxi+routify@2.18.11/node_modules/@roxi/routify/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/@roxi+routify@2.18.11/node_modules/@roxi/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/@roxi+routify@2.18.11/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/osman/pardus-dev/node_modules/.pnpm/@roxi+routify@2.18.11/node_modules/@roxi/routify/plugins/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/@roxi+routify@2.18.11/node_modules/@roxi/routify/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/@roxi+routify@2.18.11/node_modules/@roxi/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/@roxi+routify@2.18.11/node_modules:/home/osman/pardus-dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../plugins/cli.js" "$@"
else
  exec node  "$basedir/../../plugins/cli.js" "$@"
fi
